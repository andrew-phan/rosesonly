<?xml version="1.0"?>
<config>
    <modules>
        <Ant_FEF>
            <version>0.1.0</version>
        </Ant_FEF>
    </modules>
    <global>
        <models>
            <fef>
                <class>Ant_FEF_Model</class>
            </fef>
        </models>
        <blocks>
            <fef>
                <class>Ant_FEF_Block</class>
            </fef>
        </blocks>
        <resources>
            <fef_setup>
                <setup>
                    <module>Ant_FEF</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </fef_setup>
            <fef_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </fef_write>
            <fef_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </fef_read>
        </resources>
    </global>
    <adminhtml>
        <layout>
            <updates>
                <fef>
                    <file>fef.xml</file>
                </fef>
            </updates>
        </layout>
    </adminhtml>
    <admin>
        <routers>
            <FEF>
                <use>admin</use>
                <args>
                    <module>Ant_FEF</module>
                    <frontName>FEF</frontName>
                </args>
            </FEF>
        </routers>
    </admin>
</config>